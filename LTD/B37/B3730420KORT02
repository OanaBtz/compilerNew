&GENATT
.ar off
.*************************************************************
.fo left
.ti ¤ 05
:COUR10.
.***********************************************************************
.* brevnavn, roteret i venstre hjørne
.da VEND 7mm 270mm width 180mm rotate -90
.* uddata-overskrift
.da OVERSKR 20mm 110mm width 170mm
.***********************************************************************
.ar VEND on
:COUR6.
&BREVNAVN
.ar off
.***********************************************************************
.ar OVERSKR on
.us Internationalt kort inddraget i ATM
.sp
.***********************************************************************
Vedlagte kort
.sp
.tp 16mm
.se kort1 = substr &GENKTNR3 1 4
.se kort2 = substr &GENKTNR3 5 4
.se kort3 = substr &GENKTNR3 9 4
.se kort4 = substr &GENKTNR3 13 4
¤Kortnr. : &kort1 &kort2 &kort3 &kort4
.sp
.if &L'&GENIND gt 0
.an &L'&GENINDKL1 gt 0
.th er inddraget den &GENIND kl. &GENINDKL1 i pengeautomaten i
.if &L'&GENIND eq 0
.an &L'&GENINDKL1 eq 0
.th er inddraget i pengeautomaten i
.if &L'&GENIND gt 0
.an &L'&GENINDKL1 eq 0
.th er inddraget den &GENIND i pengeautomaten i
.if &L'&GENIND eq 0
.an &L'&GENINDKL1 gt 0
.th er inddraget kl. &GENINDKL1 i pengeautomaten i
.sp
.if &L'&BANKREGNR gt 0
.th ¤&BKNAVN
.th .br
.th ¤&BKAFD
.th .br
.th ¤&BKADR
.th .br
.th ¤&BKBY
.el ¤&BANKNAVN
.el .br
.el ¤&AFDNAVN
.el .br
.el ¤&AFDGADE
.el .br
.el ¤&AFDBY
.sp
.su off
.se indr = 'Kortet er inddraget som følge af'
.se spr = 'at kortet er spærret'
.se tekfejl = 'at der har været teknisk fejl i automaten'
.se ejtaget = 'at kortet ikke er taget af kunden efter præsentation'
.se defekt = 'at kortet er defekt og bør udskiftes'
.se glf = 'at kortet er glemt/fundet i automaten'
.su on
.************************** Hvis kun et modul ***************************
.******************************* Spærret ********************************
.*.da GRUND 20mm 136mm width 175mm
.*.ar GRUND on
.if &L'&GENKORTSP gt 0
.an &L'&GENTEKFEJL eq 0
.an &L'&GENEJTAGT1 eq 0
.an &L'&GENKORTDEF eq 0
.an &L'&GENKORTFUN eq 0
.an &L'&GENANDEN eq 0
.th &indr., &spr..
.th .go SLUT
.**************************** Teknisk fejl *****************************
.if &L'&GENKORTSP eq 0
.an &L'&GENTEKFEJL gt 0
.an &L'&GENEJTAGT1 eq 0
.an &L'&GENKORTDEF eq 0
.an &L'&GENKORTFUN eq 0
.an &L'&GENANDEN eq 0
.th &indr., &tekfejl..
.th .go SLUT
.**************************** Ikke taget *******************************
.if &L'&GENKORTSP eq 0
.an &L'&GENTEKFEJL eq 0
.an &L'&GENEJTAGT1 gt 0
.an &L'&GENKORTDEF eq 0
.an &L'&GENKORTFUN eq 0
.an &L'&GENANDEN eq 0
.th &indr., &ejtaget..
.th .go SLUT
.**************************** Kort defekt ******************************
.if &L'&GENKORTSP eq 0
.an &L'&GENTEKFEJL eq 0
.an &L'&GENEJTAGT1 eq 0
.an &L'&GENKORTDEF gt 0
.an &L'&GENKORTFUN eq 0
.an &L'&GENANDEN eq 0
.th &indr., &defekt..
.th .go SLUT
.*************************** Kort glemt/fundet *************************
.if &L'&GENKORTSP eq 0
.an &L'&GENTEKFEJL eq 0
.an &L'&GENEJTAGT1 eq 0
.an &L'&GENKORTDEF eq 0
.an &L'&GENKORTFUN gt 0
.an &L'&GENANDEN eq 0
.th &indr., &glf..
.th .go SLUT
.*************************** Anden årsag *******************************
.if &L'&GENKORTSP eq 0
.an &L'&GENTEKFEJL eq 0
.an &L'&GENEJTAGT1 eq 0
.an &L'&GENKORTDEF eq 0
.an &L'&GENKORTFUN eq 0
.an &L'&GENANDEN gt 0
.th &indr., &GENANDEN..
.th .go SLUT
.*************************** Intet angivet *****************************
.if &L'&GENKORTSP eq 0
.an &L'&GENTEKFEJL eq 0
.an &L'&GENEJTAGT1 eq 0
.an &L'&GENKORTDEF eq 0
.an &L'&GENKORTFUN eq 0
.an &L'&GENANDEN eq 0
.th .go SLUT
.*************************** Hvis flere valg ***************************
&indr.:
.sp
.if &L'&GENKORTSP gt 0
.th ¤&spr
.th .br
.if &L'&GENTEKFEJL gt 0
.th ¤&tekfejl
.th .br
.if &L'&GENEJTAGT1 gt 0
.th ¤&ejtaget
.th .br
.if &L'&GENKORTDEF gt 0
.th ¤&defekt
.th .br
.if &L'&GENKORTFUN gt 0
.th ¤&glf
.th .br
.if &L'&GENANDEN gt 0
.th ¤&GENANDEN
.*.go SLUT
.************************************************************************
...SLUT
.* GENEKSP      MIBAFSU1
.* GENTITEL     MIBAFSSU1
.* GENMEKSP     MIBAFSU2
.* GENMTITEL    MIBAFSSU2
.se brevs = (B37HILS)
.se B37HILS = 1
.* HVIS parametren  B37MTIT = J medtages titler
.* HVIS parametrene AFDNAVN og afdnavn EKSISTERER MEDTAGES AFDELINGSNAVN
.* PARAMETREN fixk JUSTERER MVH PÅ SIDEN - standard 43
.* ANTAL UNDERSKRIFTER (B37ANTUSK) - standard 1 (9 = ingen afslutning)
.if &E'&B37ANTUSK = 0
.th .se B37ANTUSK = 1
.if .&B37ANTUSK = .9 .go EJ9
.ti ^ 05
.* HVIS parametren  B37MTIT = J medtages titler
.if &E'&B37MTIT = 0
.th .go TIT
.if &U'&B37MTIT = J
.th .go MTIT
.el .go TIT
...TIT
.se GENTITEL ''
.se GENMTITEL ''
...MTIT
.* JUSTERING AF AFSTAND VED UNDERSKRIFTER
.if .&ÅLC < .17
.th .se T = 1
.if .&T = .1
.th .sk 3
.el .sk 4
.* OMREGN COURIER 12 til mm
.se mmt = 25
.if &E'&fixk = 0
.th .se fixk = 43
.if &E'&mmd  = 0
.th .se mmd  = 12
.se fixmm = &fixk.*&mmt./&mmd
.tp &fixmm.mm center
^Med venlig hilsen
.*sk
.*^M i d t b a n k
.if &E'&AFDNAVN = 1 .an &E'&afdnavn = 1
.th .sk
.th ^&AFDNAVN
.if .&T = .1
.th .sk 3
.el .sk 4
.if .&B37ANTUSK ne .1 .go EJ1
^&GENEKSP
.br
^&GENTITEL
...EJ1
.if .&B37ANTUSK ne .2 .go EJ2
.se MIBTAB1 = &L'&GENMEKSP - &L'&GENMTITEL
.if &MIBTAB1 > 0
.th .se mibl2 = &L'&GENMEKSP
.el .se mibl2 = &L'&GENMTITEL
.se MIBTAB2 = &L'&GENEKSP - &L'&GENTITEL
.if &MIBTAB2 > 0
.th .se mibl1 = &L'&GENEKSP
.el .se mibl1 = &L'&GENTITEL
.if &mibl2 > &mibl1
.th .se mibdiff = &mibl2 - &mibl1
.th .se mibtab1 = &fixk - &mibl2 - 2
.th .se mibtab2 = &fixk + &mibdiff + 2
.el .se mibdiff = &mibl1 - &mibl2
.el .se mibtab1 = &fixk - &mibl2 - &mibdiff - 2
.el .se mibtab2 = &fixk + 2
.se mibtab1m = &mibtab1 * &mmt / &mmd
.se mibtab2m = &mibtab2 * &mmt / &mmd
.tp set &mibtab1m.mm &mibtab2m.mm
^&GENMEKSP ^&GENEKSP
.br
^&GENMTITEL ^&GENTITEL
...EJ2
.tp
.br
...EJ9
