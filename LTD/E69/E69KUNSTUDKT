.se skudar = 365
.se nyddmm = &fdag..&fmdr.
.se nyfdma = &nyddmm..&faarh.
.se ndmm = &fmdr*&fdprmd.
.se ndaa = &faarh*&skudar.
.se nddm = &fdag.+&ndmm.
.se ndage = &fdag.+&ndmm.+&ndaa.
.se dmm = &mdr*&ddprmd.
.se daa = &aar*365
.se ddage = &dag.+&dmm.+&daa.
.se ddiff = &ddage.-&ndage.
.se dalder = &ddiff./&skudar.
