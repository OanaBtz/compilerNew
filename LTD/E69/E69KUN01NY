.***** HER DELES DAGSDATO OP I DATO, MÅNED OG ÅR **************************
.se aar = substr &DATO 7 4
.se mdr = substr &DATO 4 2
.se dag = substr &DATO 1 2
.***** HER DELES KUNDENR OP I DATO, MÅNED OG ÅR ***************************
.se lnr = substr &KUNDENR 7 4
.se faar = substr &KUNDENR 5 2
.se fmdr = substr &KUNDENR 3 2
.se fdag = substr &KUNDENR 1 2
.if x&fdag = x00
.th .go CVRKUN
.***** FIND ÅRHUNDREDE FOR FØDSELSÅR (KUNDENR) ****************************
.if x&lnr < x4000
.th .se faarh = 1900 + &faar.
.th .se cprid = 1-4
.if x&lnr > x3999
.an x&lnr < x5000
.an x&faar < x37
.th .se faarh = 2000 + &faar.
.th .se cprid = 5
.if x&lnr > x3999
.an x&lnr < x5000
.an x&faar > x36
.th .se faarh = 1900 + &faar.
.th .se cprid = 6
.if x&lnr > x4999
.an x&lnr < x6000
.an x&faar < x58
.th .se faarh = 2000 + &faar.
.th .se cprid = 7
.if x&lnr > x4999
.an x&lnr < x6000
.an x&faar > x57
.th .se faarh = 1800 + &faar.
.th .se cprid = 8
.if x&lnr > x5999
.an x&lnr < x7000
.an x&faar < x58
.th .se faarh = 2000 + &faar.
.th .se cprid = 9
.if x&lnr > x5999
.an x&lnr < x7000
.an x&faar > x57
.th .se faarh = 1800 + &faar.
.th .se cprid = 10
.if x&lnr > x6999
.an x&lnr < x8000
.an x&faar < x58
.th .se faarh = 2000 + &faar.
.th .se cprid = 11
.if x&lnr > x6999
.an x&lnr < x8000
.an x&faar > x57
.th .se faarh = 1800 + &faar.
.th .se cprid = 12
.if x&lnr > x7999
.an x&lnr < x9000
.an x&faar < x58
.th .se faarh = 2000 + &faar.
.th .se cprid = 13
.if x&lnr > x7999
.an x&lnr < x9000
.an x&faar > x57
.th .se faarh = 1800 + &faar.
.th .se cprid = 14
.if x&lnr > x8999
.an x&lnr <= x9999
.an x&faar < x37
.th .se faarh = 2000 + &faar.
.th .se cprid = 15
.if x&lnr > x8999
.an x&lnr <= x9999
.an x&faar > x36
.th .se faarh = 1900 + &faar.
.th .se cprid = 16
.***** BEREGN ANTAL DAGE PR MD FOR FØDSELSDAG *****************************
.if x&fmdr = x01
.th .se fdprmd = 31
.if x&fmdr = x02
.th .se fdprmd = 28
.if x&fmdr = x03
.th .se fdprmd = 31
.if x&fmdr = x04
.th .se fdprmd = 30
.if x&fmdr = x05
.th .se fdprmd = 31
.if x&fmdr = x06
.th .se fdprmd = 30
.if x&fmdr = x07
.th .se fdprmd = 31
.if x&fmdr = x08
.th .se fdprmd = 31
.if x&fmdr = x09
.th .se fdprmd = 30
.if x&fmdr = x10
.th .se fdprmd = 31
.if x&fmdr = x11
.th .se fdprmd = 30
.if x&fmdr = x12
.th .se fdprmd = 31
.***** BEREGN ANTAL DAGE PR MD FOR DAGS DATO ******************************
.if x&mdr = x01
.th .se ddprmd = 31
.if x&mdr = x02
.th .se ddprmd = 28
.if x&mdr = x03
.th .se ddprmd = 31
.if x&mdr = x04
.th .se ddprmd = 30
.if x&mdr = x05
.th .se ddprmd = 31
.if x&mdr = x06
.th .se ddprmd = 30
.if x&mdr = x07
.th .se ddprmd = 31
.if x&mdr = x08
.th .se ddprmd = 31
.if x&mdr = x09
.th .se ddprmd = 30
.if x&mdr = x10
.th .se ddprmd = 31
.if x&mdr = x11
.th .se ddprmd = 30
.if x&mdr = x12
.th .se ddprmd = 31
.**************************************************************************
.****** SKUDÅR KAN ENDNU IKKE BEREGNES ************************************
.se skudar = 365
.****** FØDSELSDATO NED AAAA DANNES ***************************************
.se nyddmm = &fdag..&fmdr.
.se nyfdma = &nyddmm..&faarh.
.****** ALDER I ANTAL DAGE FOR FØDSELSDAG *********************************
.se ndmm = &fmdr*&fdprmd.
.se ndaa = &faarh*&skudar.
.se nddm = &fdag.+&ndmm.
.se ndage = &fdag.+&ndmm.+&ndaa.
.****** ANTAL DAGE FOR DAGS DATO ******************************************
.se dmm = &mdr*&ddprmd.
.se daa = &aar*365
.se ddage = &dag.+&dmm.+&daa.
.****** FORSKEL I DAGE MELLEM DAGS DATO OG FØDSLESDATO ********************
.se ddiff = &ddage.-&ndage.
.****** ALDER (ÅR) ********************************************************
.se dalder = &ddiff./&skudar.
.go CPREND
...CVRKUN
.******dalder SKAL HAVE VÆRDI SELV FOR CVR *******************************
.se dalder = -1
...CPREND
