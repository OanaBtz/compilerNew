.if &GENKR eq 0,00
.th .go EJFREKV
.if &L'&GENLØNINDB = 0
.th .go TESTBEL
.if x&GENLØNINDB < x1
.or x&GENLØNINDB > x6
.th .mg /T/Frekvens skal have værdien 1-6/
.if x&GENLØNINDB ne x1
.th .go AAR1SL
...TESTBEL
.if &L'&RESULTA gt 9
.th .mg /T/BELØB STØRRE END 0.00/
.if &L'&RESULTA lt 9
.th .go EJFREKV
.if X&RESULTA gt X36.500,00
.th .mg /T/BELØB STØRRE END 0.00/
.el .go EJFREKV
...AAR1SL
.if x&GENLØNINDB ne x2
.th .go HALV1SL
