.*TEST BELØB 1*************************************************************
.se bel1 = 0
.if &L'&GENMØNTKD1 > 0
.th .se bel1a = &bel1 + 1
.el .se bel1a = &bel1
.if &L'&GENBELØB11 > 0
.th .se bel1b = &bel1a + 1
.el .se bel1b = &bel1a
.if &L'&GENPCTPA1 > 0
.th .se bel1c = &bel1b + 1
.el .se bel1c = &bel1b
.if &L'&GENTP1 > 0
.th .se bel1d = &bel1c + 1
.el .se bel1d = &bel1c
.if &L'&GENTEXT1 > 0
.th .se bel1e = &bel1d + 1
.el .se bel1e = &bel1d
.if x&bel1e = x0
.or x&bel1e = x5
.th .go OPRYK2
.el .mg/T/Der mangler oplysninger vedr. 1. opr. panteret/
.**************************************************************************
.*TEST BELØB 2**************************************************
...OPRYK2
.se bel2 = 0
.if &L'&GENMØNTKD2 > 0
.th .se bel2a = &bel2 + 1
.el .se bel2a = &bel2
.if &L'&GENBELØB12 > 0
.th .se bel2b = &bel2a + 1
.el .se bel2b = &bel2a
.if &L'&GENPCTPA2 > 0
.th .se bel2c = &bel2b + 1
.el .se bel2c = &bel2b
.if &L'&GENTP2 > 0
.th .se bel2d = &bel2c + 1
.el .se bel2d = &bel2c
.if &L'&GENTEXT2 > 0
.th .se bel2e = &bel2d + 1
.el .se bel2e = &bel2d
.if x&bel2e = x0
.or x&bel2e = x5
.th .go OPRYK3
.el .mg/T/Der mangler oplysninger vedr. 2. opr. panteret/
.*********************************************************************
.*TEST BELØB 3***********
...OPRYK3
.se bel3 = 0
.if &L'&GENMØNTKD3 > 0
.th .se bel3a = &bel3 + 1
.el .se bel3a = &bel3
.if &L'&GENBELØB13 > 0
.th .se bel3b = &bel3a + 1
.el .se bel3b = &bel3a
.if &L'&GENPCTPA3 > 0
.th .se bel3c = &bel3b + 1
.el .se bel3c = &bel3b
.if &L'&GENTP3 > 0
.th .se bel3d = &bel3c + 1
.el .se bel3d = &bel3c
.if &L'&GENTEXT3 > 0
.th .se bel3e = &bel3d + 1
.el .se bel3e = &bel3d
.if x&bel3e = x0
.or x&bel3e = x5
.th .go OVER
.el .mg/T/Der mangler oplysninger vedr. 3. opr. panteret/
.********************************************************
...OVER
.if x&bel1e = x5
.or x&bel2e = x5
.or x&bel3e = x5
.th .go OVER1
.el Der er ingen foranstående hæftelser.
.el .go SLUT
...OVER1
.*************************************************
