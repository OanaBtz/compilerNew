.if &GENPRIERHV ne 2
.th .go ERHOVSL1
.if &GENTYP = 1
.or &GENERHVTYP = 1
.th .go ERHOV1
.el .go ERHOVSL1
...ERHOV1
.**************************************************************
:COUR10.
.ti ¤ 05
.fo left
.**************************************************************
.da OVERSK 20mm 32mm width 175mm
.da VEND 7mm 270mm width 180mm rotate -90
.**************************************************************
.ar OVERSK on
:SWISS12.
