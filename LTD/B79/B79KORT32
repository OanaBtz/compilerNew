.* I BECKORT33 TESTES PÅ B11KTTYPE
.************************************************************************
.if &GENPRIERHV ne 1
.th .go PRIOVSL1
.if &GENTYP = 1
.or &GENPRIVTYP = 1
.or &B11KTTYPE = 60
.th .go PRIOV1
.el .go PRIOVSL1
...PRIOV1
.**************************************************************
:COUR10.
.ti ¤ 05
.fo left
.**************************************************************
.da OVERSK 20mm 32mm width 175mm
.da VEND 7mm 270mm width 180mm rotate -90
.**************************************************************
